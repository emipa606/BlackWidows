  <HediffDef ParentName="AddedBodyPartBase">
    <defName>BionicRightPedipalp</defName>
    <label>bionic right pedipalp</label>
    <description>This bionic pedipalp renders what is otherwise a rather inoffensive part of Widow biology into a weapon of incredible destructive potential. Can stun enemies in close combat.</description>
    <comps>
      <li Class="HediffCompProperties_VerbGiver">
        <tools>
          <li>
            <label>bionic right pedipalp</label>
            <capacities>
              <li>BW_Puncture</li>
            </capacities>
            <power>15</power>
            <cooldownTime>2.3</cooldownTime>
            <chanceFactor>0.25</chanceFactor>
          </li>
          <li>
            <label>bionic right pedipalp</label>
            <capacities>
              <li>Stab</li>
            </capacities>
            <power>15</power>
            <cooldownTime>2.3</cooldownTime>
            <chanceFactor>0.5</chanceFactor>
          </li>
        </tools>
      </li>
    </comps>
    <spawnThingOnRemoved>BionicPedipalp</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
      <partEfficiency>1.0</partEfficiency>
    </addedPartProps>
    <!--<nakedBodyGraphicsPath>Things/Widows/WidowsBodyBionic/</nakedBodyGraphicsPath>-->
  </HediffDef>
