      </li>
      <li>
        <defName>Construction</defName>
        <amount>2</amount>
      </li>
    </skillGains>
    <spawnCategories>
      <li>BlackWidow_Backstory</li>
      <li>BlackWidowOfficer</li>
    </spawnCategories>
    <!--<saveKeyIdentifier>BlackWidowIDOfficer</saveKeyIdentifier>-->
  </AlienRace.BackstoryDef>
  <AlienRace.BackstoryDef>
    <defName>Black_Widow_Nurse</defName>
    <title>Nurse</title>
    <titleShort>Nurse</titleShort>
    <baseDescription>{PAWN_nameDef} made a living as a nurse working hospice care in a non-Black Widows' controlled glitter world.
</baseDescription>
    <bodyTypeMale>Female</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
