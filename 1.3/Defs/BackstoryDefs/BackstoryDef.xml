  <AlienRace.BackstoryDef>
    <defName>Black_Widow_NavyCook</defName>
    <title>Navy cook</title>
    <titleShort>Cook</titleShort>
    <baseDescription>{PAWN_nameDef} served as a chef in the Black Widow navy.
</baseDescription>
    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>
    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Cooking</defName>
